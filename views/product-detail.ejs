                      <div class="images-grids">
                    <% proImages.forEach((p_image,index) => { %>     
                        <div class="image-item" data-index="<%= index+1 %>">
                          <img src="<%= p_image.p_images %>" class="d-img" alt="">
                        </div>
                    <% }) %>     
                      </div>
                    </div>
                  </div>
