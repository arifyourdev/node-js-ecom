                        <% if (proImages.length> 0) { %>
                          <% proImages.forEach(function(p_image, index) { %>
                            <div class="d-item" data-index="<%= index %>">
                              <img src="<%= p_image.p_images %>"
                                data-zoom-image="<%= p_image.p_images %>" class="pd-img" alt="" />
                            </div>
                            <% }); %>
                              <% } else { %>
                                <div class="d-item" data-index="0">
                                  <img src=".<%= p_image.p_images %>"
                                    data-zoom-image=".<%= p_image.p_images %>" alt="" />
                                </div>
                                <% } %>
                      </div>
                     
                    </div>

                  </div>
                </div>
              </div>
