                        </a>
                      </li>

                    </ul>
                  </div>
        
                    <div class="product-size-list">
                      <div class="pro--text">
                        <h6 class="size-selection">Select Size</h6>
                      </div>
                      <ul class="flex gap-4">
                        <% sizes.forEach(size => { %>    
                            <li class="relative">
                                <button 
                                    class="size-button 
                                    <% if (size.quantity === 0) { %> size-disabled <% } else { %> bg-gray-100 <% } %>"
                                    data-size="<%= size.size %>" 
                                    data-quantity="<%= size.quantity %>"
                                    data-selected="false"
                                    <% if (size.quantity === 0) { %> disabled <% } %>
                                    <% if (size.quantity < 5) { %>   <%= size.quantity %> items available" <% } %>
                                    >
                                    <%= size.size %>
                                </button>
                                <% if (size.quantity > 0 && size.quantity < 5) { %>
                                    <div class="tooltip hidden absolute left-1/2 transform -translate-x-1/2 bottom-full mb-2 px-4 py-2 bg-gray-800 text-white text-sm rounded opacity-0 transition-opacity duration-300">
                                        Hurry up! Only <%= size.quantity %> items left
                                    </div>
                                <% } %>
                            </li>
                        <% }); %>
